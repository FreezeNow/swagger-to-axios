    "swagger2openapi": "^7.0.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.199",
    "@types/node": "^16.9.1",
    "@types/swagger2openapi": "^7.0.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.6.1",
    "mock-fs": "^5.2.0",
    "pactum": "^3.5.1",
    "prettier": "^2.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
