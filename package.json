{
  "name": "swagger-to-axios",
  "version": "1.0.14",
  "description": "将swagger转换成axios可用的api文件",
  "main": "lib/index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/FreezeNow/swagger-to-axios.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
